{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": [".next/**"]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "outputs": [".next/**"]
        },
        "db:generate": {
